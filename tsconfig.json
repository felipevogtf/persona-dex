{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@models/*": ["models/*"],
      "@data/*": ["data/*"],
      "@layouts/*": ["layouts/*"],
      "@components/*": ["components/*"],
      "@utils/*": ["utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
